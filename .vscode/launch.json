  "version": "0.2.0",
  "configurations": [
    {
      "name": "Tests (Yunit)",
      "type": "autohotkey",
      "request": "launch",
      "program": "${workspaceFolder}\\tests\\YunitTestrunner.ahk",
      "args": [],
      "port": "9002-9010",
      "variableCategories": "recommend",
      "useAnnounce": "error",
      "useDebugDirective": false,
      "useAutoJumpToError": true,
      "usePerfTips": "{{elapsedTime_ms}}ms elapsed.",
      "useOutputDebug": {
        "useTrailingLinebreak": true
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Test Stdout",
      "type": "autohotkey",
      "request": "launch",
      "program": "${workspaceFolder}\\doc\\ExampleStdout.ahk",
      "args": [],
      "port": "9002-9010",
      "variableCategories": "recommend",
      "useAnnounce": "error",
      "useDebugDirective": false,
      "useAutoJumpToError": true,
      "usePerfTips": "{{elapsedTime_ms}}ms elapsed.",
      "useOutputDebug": {
        "useTrailingLinebreak": true
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Current file",
      "type": "autohotkey",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "port": "9002-9010",
      "variableCategories": "recommend",
      "useAnnounce": "error",
      "useDebugDirective": false,
      "useAutoJumpToError": true,
      "usePerfTips": "{{elapsedTime_ms}}ms elapsed.",
      "useOutputDebug": {
        "useTrailingLinebreak": true
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    
  ]
}